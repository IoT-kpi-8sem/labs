generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AgentMessage {
  id              String   @id @default(ulid())
  clientId        String 
  accelerometer   Json @db.Json
  gps             Json @db.Json
  roadState       String?
  time            DateTime 
}